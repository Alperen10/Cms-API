# Server Configuration
SERVER_PORT=8080
SERVER_HOST=localhost

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=123456
DB_NAME=cms_db
DB_SSL_MODE=disable

# JWT Configuration
JWT_SECRET=your_jwt_secret_key
